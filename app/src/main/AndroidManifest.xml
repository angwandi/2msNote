<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.demad.a2msnote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_note_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NoteActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddNoteActivity"
            android:label="Add Note"
            android:parentActivityName=".NoteActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".EditNoteActivity"
            android:label="Edit Note"
            android:parentActivityName=".AddNoteActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".DetailScreenNoteActivity"
            android:label="Note Detail Screen"
            android:parentActivityName=".NoteActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".ConsssActivity"
            android:label="@string/title_activity_consss"></activity>
    </application>
</manifest>
